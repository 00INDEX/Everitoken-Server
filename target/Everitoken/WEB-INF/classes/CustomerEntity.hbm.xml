<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="everitoken.entity.CustomerEntity" table="customer" schema="everitoken">
        <id name="customerUid" column="customer_uid"/>
        <property name="customerUsername" column="customer_username"/>
        <property name="customerPassword" column="customer_password"/>
        <property name="customerEmail" column="customer_email"/>
        <property name="customerSex" column="customer_sex"/>
        <property name="customerName" column="customer_name"/>
        <property name="customerIdnumber" column="customer_idnumber"/>
        <property name="customerPrivateKey" column="customer_private_key"/>
    </class>
</hibernate-mapping>