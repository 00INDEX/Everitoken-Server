<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="everitoken.entity.ProducerEntity" table="producer" schema="everitoken">
        <id name="producerUid" column="producer_uid">
            <generator class="identity"/>
        </id>
        <property name="producerEmail" column="producer_email"/>
        <property name="producerName" column="producer_name"/>
        <property name="producerPrivateKey" column="producer_private_key"/>
        <property name="authorized" column="authorized"/>
        <property name="producerChnCode" column="producer_CHNCode"/>
    </class>
</hibernate-mapping>