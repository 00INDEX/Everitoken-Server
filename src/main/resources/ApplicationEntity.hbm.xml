<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="everitoken.entity.ApplicationEntity" table="application" schema="everitoken">
        <id name="uid" column="uid">
            <generator class="identity"/>
        </id>
        <property name="applicantUid" column="applicant_uid"/>
        <property name="applicationTime" column="application_time"/>
        <property name="applicationDocuments" column="application_documents"/>
        <property name="authorized" column="authorized"/>
    </class>
</hibernate-mapping>