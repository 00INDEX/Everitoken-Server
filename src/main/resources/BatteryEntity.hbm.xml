<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="everitoken.entity.BatteryEntity" table="battery" schema="everitoken">
        <id name="uid" column="uid">
            <generator class="identity"/>
        </id>
        <property name="batteryCapacity" column="battery_capacity"/>
        <property name="batteryType" column="battery_type"/>
        <property name="batteryMaxVoltage" column="battery_max_voltage"/>
        <property name="batteryAverageTemperature" column="battery_average_temperature"/>
        <property name="batteryChgCycles" column="battery_CHG_cycles"/>
        <property name="batteryName" column="battery_name"/>
    </class>
</hibernate-mapping>